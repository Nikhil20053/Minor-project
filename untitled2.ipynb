import numpy as np

#Function to assign grade based on average marks
def assign_grade(avg):
  if avg > 85:
    return "A"
  elif avg >= 70:
    return "B"
  elif avg >= 50:
    return "C"
  else:
    return "Fail"
#Take number of student as input
n = int(input("Enter number of students: "))

marks_list = [] #Empty list to store marks
#Taking marks input using the loops
for i in range(n):
  marks = float(input(f"Enter the marks of student {i+1}: "))
  marks_list.append(marks)

  #coverting list into numpy array
marks_array = np.array(marks_list)

  #calculation using numpy functions
total_marks = marks_array.sum()
average_marks = marks_array.mean()
highest_marks = marks_array.max()
lowest_marks = marks_array.min()

#Assign grade
grade = assign_grade(average_marks)

#Dispaly results or output
print("\n student marks analysis")
print("Marks: ",marks_array)
print("Total marks: ", total_marks)
print("Average marks: ", average_marks)
print("Higest marks: ", highest_marks)
print("Lowest marks: ", lowest_marks)
print("Grade: ",grade)